/* pages/seach/seach.wxss */
.search-input{
  width: 450rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #666;
  border-radius: 10rpx;
  padding-left: 70rpx;
  background: #fff;
}
.seachbg{
  position: fixed;
  top: 0;
  background: #EFEFF4;
  z-index: 999;
  width: 100%;
  border-bottom:1px solid #fff; 
}
.seach{
  width: 650rpx;
  margin: 0 auto;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
}
.seachtext{
  color: #2d78f4;
  font-size: 34rpx;
  margin-left: 40rpx;
}
.inicon{
  width: 40rpx;
  height: 40rpx;
  background: url(data:image/png;base64,) no-repeat;
}
.inpos{
  background-position: -124rpx -1rpx;
  position:absolute;
  background-size: 167rpx 45rpx;
  top:31rpx;
  left:28rpx;
}
.list{
  width: 100%;
  margin-top: 135rpx;
}
.list_pro{
  width: 100%;
  height: 150rpx;
  background: #EFEFF4;
  margin-top: 30rpx;
  padding-top: 30rpx;
  position: relative;
}
.list_title{
  font-size: 35rpx;
  color: #000;
  margin-left: 30rpx;
}
.recovery{
  display: block;
  margin-left: 30rpx;
  color: #666;
  margin-top: 30rpx;
}
.list_icon{
  position:absolute;
  background-size: 223rpx 60rpx;
  top:70rpx;
  right:28rpx;
  transform: scale(1.3);
  background-position: -57rpx -8rpx;
}
.errtext{
  width: 100%;
  position: absolute;
  top: 0;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 28rpx;
  background: #e71c35;
  color: #fff;
  z-index: 9999;
  text-align: center;
}
.home{
  width:85rpx;
  height:85rpx;
  position:fixed;
  top:500rpx;
  right:10rpx;
  background:#ffff;
  border-radius:50%;
  text-align:center;
  font-size:24rpx;
  color:#2d78f4;
  border: 1rpx solid #2d78f4;
  overflow: hidden;
}
.homeimg{
  width: 87rpx;
  height: 74rpx;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size:65%; 
  position: absolute;
  top:6rpx;
  left:17rpx;
}
.hometext{
  position:absolute;
  top:13rpx;
  left:18rpx;
}